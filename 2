из  случайного  импорта  randint
def  пузырь ( массив ):
      для  i  в  диапазоне ( N - 1 ):
          для  j  в  диапазоне ( N - i - 1 ):
              если  массив [ j ] > массив [ j + 1 ]:
                  бафф  =  массив [ j ]
                  массив [ j ] =  массив [ j + 1 ]
                  массив [ j + 1 ] = бафф

N  =  input ( "Наберите необходимое количество случайных чисел;" )

попробуйте :
    б = int ( N )
кроме :
    print ( "Введите натуральное число:" )
    выход ()

N = int ( N )

а = []
для  i  в  диапазоне ( N ):
    а . добавить ( рандинт ( 1 , 1000 ))

пузырь ( а )
печать ( а )
