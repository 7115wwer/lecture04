 случайный импорт

N  =  input ( "Наберите количество рандомных чисел:" )
N  =  int ( N )

# Сортируем от нуля до n-1 с помощью Богсорт
def  bogoSort ( a ):
    п  =  len ( а )
    while ( is_sorted ( a ) ==  False ):
        перемешать ( а )

# Делаем проверка
def  is_sorted ( а ):
    п  =  len ( а )
    для  i  в  диапазоне ( 0 , n  -  1 ):
        если ( a [ i ] >  a [ i  +  1 ]):
            вернуть  ложь
        вернуть  True

def  shuffle ( а ):
    п  =  len ( а )
    для  i  в  диапазоне ( 0 , n ):
        r  =  случайный . рандинт ( 0 , п  -  1 )
        а [ я ], а [ г ] =  а [ г ], а [ я ]

# Даем изменяемое количество N членов из промежутка от 0 до 1000
а  = []
для  i  в  диапазоне ( N ):
    а . добавить ( случайный . randint ( 1 , 1000 ))

bogoSort ( а )
print ( "отсортировано:" )
для  i  в  диапазоне ( len ( a )):
    print ( a [ i ])
